{
    "containerDefinitions": [
            {
                "name": "nginxdemos-hello",
                "image": "nginxdemos/hello",
                "cpu": 0,
                "portMappings": [
                    {
                        "containerPort": 80,
                        "hostPort": 80,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,
                "environment": [],
                "mountPoints": [],
                "volumesFrom": [],
                "logConfiguration": {
                    "logDriver": "awslogs",
                    "options": {
                        "awslogs-group": "/ecs/nginxdemos-hello",
                        "awslogs-region": "nginxdemos/hello",
                        "awslogs-stream-prefix": "ecs"
                    }
                }
            }
    ],
    "family": "nginxdemos-hello",
    "taskRoleArn": "arn:aws:iam::333854148048:role/ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::333854148048:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "cpu": "1024",
    "memory": "2048"
}
